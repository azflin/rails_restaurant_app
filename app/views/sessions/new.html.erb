<h1>Login</h1>

<%= form_tag sessions_path do %>

	<% if flash[:notice] %>
		<p><%= flash[:notice] %></p>
	<% end %>

	<p>
		<label>Email:</label><br>
		<%= text_field_tag :email %>
	</p>

	<p>
		<label>Password:</label><br>
		<%= password_field_tag :password %>
	</p>

	<p>
		<%= submit_tag "Login" %>
	</p>

<% end %>


